def insertion_sort(nums):
    #loops from the second index of the list; first index is already sorted
    for i in range(1, len(nums)): 
        current = nums[i] 
        j = i - 1 
        while (j >= 0 and nums[j] > current): #only runs when previous index > current
            nums[j+1] = nums[j]
            j -= 1 
        nums[j+1] = current
    return nums 

